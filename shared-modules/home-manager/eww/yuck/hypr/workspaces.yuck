(deflisten workspacesArray "./scripts/hypr/workspaces.sh")

(defwidget workspaces [wsscreen]
  (eventbox
    (box :class "workspaces"
      (for i in {workspacesArray[wsscreen].workspaces}
        (button
          :onclick "hyprctl dispatch workspace ${i.id}"
          :class "${i.class}"
          "${i.name}")))))