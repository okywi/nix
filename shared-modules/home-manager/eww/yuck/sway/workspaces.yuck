(deflisten workspacesArray :initial "" `./scripts/sway/workspaces.sh`)

(defwidget workspaces [wsscreen]
(box    :class "workspaces"
        :orientation "h"
        :space-evenly true
        :halign "start"
    
    (for entry in {workspacesArray[wsscreen]}
        (button :onclick `swaymsg workspace "${entry.num}"`
                :class {entry.class}
        `${entry.icon}`))))